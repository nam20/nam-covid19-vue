          <template v-slot:title>\r\n                            유튜브 관련 영상\r\n                        </template>\r\n                        <template v-slot:body>\r\n                            <v-card \r\n                            v-for=\"item in youtubeList\" \r\n                            :key=\"item\" \r\n                            class=\"px-2\" \r\n                            tile outlined \r\n                            :href=\"`https://www.youtube.com/watch?v=${item.id.videoId}`\" \r\n                            target=\"_blank\">\r\n                                <v-row align=\"center\">\r\n                                    <v-col cols=\"4\">\r\n                                        <v-img :src=\"item.snippet.thumbnails.medium.url\"></v-img>\r\n                                    </v-col>\r\n                                    <v-col cols=\"8\">\r\n                                        <v-card-title v-text=\"item.snippet.title\" class=\"mb-2\"></v-card-title>\r\n                                        <v-card-subtitle v-text=\"item.snippet.description\"></v-card-subtitle>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                \r\n                            </v-card>\r\n                        </template>\r\n                    </chart-card>\r\n                </v-col>\r\n                <v-col cols=\"12\" >\r\n                    <chart-card>\r\n                        <template v-slot:title>\r\n                            전염병 통계 비교\r\n                        </template>\r\n                        <template v-slot:body>\r\n                            <v-simple-table class=\"my-3\" >\r\n                                <thead>\r\n                                    <tr>\r\n                                        <th><h3>병명</h3></th>\r\n                                        <th><h2>코로나 19</h2> </th>\r\n                                        <th><h2>MERS</h2> </th>\r\n      