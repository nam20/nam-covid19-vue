= confirmed.slice(confirmed.length-2)\r\n            recovered = recovered.slice(recovered.length-2)\r\n            deaths = deaths.slice(deaths.length-2)\r\n\r\n            this.todayKoreaTotal = {\r\n                totalConfirmed : this.numberCommas(confirmed[1]),\r\n                totalDeaths : this.numberCommas(deaths[1]),\r\n                totalRecovered : this.numberCommas(recovered[1]),\r\n                totalCritical: this.numberCommas((deaths[1] / confirmed[1] * 100).toFixed(1)),\r\n                newConfirmed : this.numberCommas(confirmed[1] - confirmed[0]),\r\n                newDeaths : this.numberCommas(deaths[1] - deaths[0]),\r\n                newRecovered : this.numberCommas(recovered[1] - recovered[0])\r\n            }\r\n        },\r\n        numberCommas(input){\r\n            return input.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\")\r\n        },\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.col p.number {\r\n    font-size: 22px;\r\n    font-weight: 500;\r\n}\r\n\r\n.col .covid {\r\n    color: red;\r\n}\r\n\r\n.col .recovered {\r\n    color: rgb(34, 182, 23);\r\n}\r\n\r\n</style>"]}]}